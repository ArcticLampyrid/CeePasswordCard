<Window x:Class="CeePasswordCard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CeePasswordCard"
        mc:Ignorable="d"
        Title="高考动态口令卡辅助查询工具" Height="240" Width="280" Topmost="True">
    <DockPanel>
        <DockPanel DockPanel.Dock="Bottom">
            <Label DockPanel.Dock="Left">坐标：</Label>
            <Button DockPanel.Dock="Right" Name="QueryButton" Click="QueryButton_Click" IsDefault="True">查询</Button>
            <TextBox Name="PosTextBox" VerticalContentAlignment="Center"></TextBox>
        </DockPanel>
        <TextBox Name="ContentTextBox" IsReadOnly="True" HorizontalScrollBarVisibility="Visible" VerticalScrollBarVisibility="Visible">
            
        </TextBox>
    </DockPanel>
</Window>
